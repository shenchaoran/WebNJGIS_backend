        "@types/jquery": "^2.0.41",
        "@types/lodash": "^4.14.63",
        "@types/morgan": "^1.7.32",
        "@types/nodemailer": "^1.3.32",
        "@types/request": "^2.47.0",
        "@types/request-promise": "^4.1.41",
        "@types/supertest": "^2.0.0",
        "concurrently": "^3.4.0",
        "del": "^3.0.0",
        "gulp": "^3.9.1",
        "gulp-cache": "^1.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-jshint": "^2.1.0",
        "gulp-notify": "^3.2.0",
        "gulp-rename": "^1.4.0",
        "gulp-uglifyes": "^0.2.1",
        "gulp-util": "^3.0.8",
        "jshint-stylish": "^2.2.1",
